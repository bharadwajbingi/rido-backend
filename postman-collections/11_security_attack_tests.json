    {
      "name": "\ud83d\udee1\ufe0f SQL Injection #1",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": "{{base_url}}/auth/login",
        "body": {
          "mode": "raw",
          "raw": "{\n    \"username\": \"admin' OR '1'='1\",\n    \"password\": \"test\"\n}"
        }
      },
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "type": "text/javascript",
            "exec": [
              "setTimeout(() => {}, 300);"
            ]
          }
        },
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "pm.test('SQLi Blocked', () => pm.expect(pm.response.code).to.be.oneOf([400, 401, 423, 429]));"
            ]
          }
        }
      ]
    },
