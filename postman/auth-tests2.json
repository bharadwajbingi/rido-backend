            "body": {
              "mode": "raw",
              "raw": "{ \"refreshToken\": \"{{refreshToken}}\" }"
            }
          },
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "const currentRt = pm.collectionVariables.get(\"refreshToken\");",
                  "pm.collectionVariables.set(\"oldRefreshToken\", currentRt);"
                ]
              }
            },
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test(\"Status is 200 OK on refresh\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "pm.test(\"Refresh returns new access & refresh tokens\", function () {",
                  "    var r = pm.response.json();",
                  "    pm.expect(r.accessToken).to.be.a(\"string\");",
                  "    pm.expect(r.refreshToken).to.be.a(\"string\");",
                  "    pm.collectionVariables.set(\"accessToken\", r.accessToken);",
                  "    pm.collectionVariables.set(\"refreshToken\", r.refreshToken);",
                  "});"
                ]
              }
            }
          ]
        },
