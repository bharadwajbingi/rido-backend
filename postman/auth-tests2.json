{
  "info": {
    "name": "Rido Auth Service - Full Random User Suite",
    "_postman_id": "rido-auth-suite-random",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8081"
    },
    {
      "key": "username",
      "value": ""
    },
    {
      "key": "password",
      "value": ""
    },
    {
      "key": "accessToken",
      "value": ""
    },
    {
      "key": "refreshToken",
      "value": ""
    },
    {
      "key": "oldRefreshToken",
      "value": ""
    }
  ],
  "item": [
    {
      "name": "0. SETUP (Random User Init)",
      "item": [
        {
          "name": "0.1 Initialize Random Username/Password",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/auth/check-username?username={{username}}"
          },
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "const uname = 'user_' + pm.variables.replaceIn('{{$timestamp}}') + '_' + pm.variables.replaceIn('{{$randomInt}}');",
                  "const pwd = 'P@ss_' + pm.variables.replaceIn('{{$randomInt}}');",
                  "pm.collectionVariables.set('username', uname);",
                  "pm.collectionVariables.set('password', pwd);",
                  "console.log('Using username=' + uname + ' password=' + pwd);"
                ]
              }
            },
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test(\"Status is 200 or 404\", function () {",
                  "    pm.expect([200,404]).to.include(pm.response.code);",
                  "});"
                ]
              }
            }
          ]
        }
      ]
    },
    {
