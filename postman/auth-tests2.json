              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test(\"Status is 200 OK on /auth/me with valid token\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "pm.test(\"Response has id and username\", function () {",
                  "    var r = pm.response.json();",
                  "    pm.expect(r.id).to.be.a(\"string\");",
