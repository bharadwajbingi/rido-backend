    {
      "name": "5. REFRESH TOKEN",
      "item": [
        {
          "name": "5.1 Refresh Success (Rotate Tokens)",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/auth/refresh",
            "header": [
